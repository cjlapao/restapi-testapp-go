{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/main.go",
      "env": {
        "RESTAPI_MONGO_CONNECTION_STRING": "${input:mongoDbConnectionString}",
        "RESTAPI_API_PORT": "10000"
      },
      "args": []
    }
  ],
  "inputs": [
    {
      "id": "mongoDbConnectionString",
      "type": "promptString",
      "description": "MongoDB Connection String"
    }
  ]
}